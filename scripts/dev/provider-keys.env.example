# Local-only provider keys for live smoke validation.
# Copy to `.tau/provider-keys.env` (gitignored) and fill values.

# Core providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GEMINI_API_KEY=

# OpenAI-compatible aliases
OPENROUTER_API_KEY=
DEEPSEEK_API_KEY=
XAI_API_KEY=
MISTRAL_API_KEY=
GROQ_API_KEY=

# Optional Tau-prefixed aliases (also supported)
TAU_OPENROUTER_API_KEY=
TAU_DEEPSEEK_API_KEY=

# Optional API-base overrides
TAU_OPENAI_API_BASE=https://api.openai.com/v1
TAU_OPENROUTER_API_BASE=https://openrouter.ai/api/v1
TAU_DEEPSEEK_API_BASE=https://api.deepseek.com
TAU_XAI_API_BASE=https://api.x.ai/v1
TAU_MISTRAL_API_BASE=https://api.mistral.ai/v1
TAU_GROQ_API_BASE=https://api.groq.com/openai/v1

# Optional model overrides
TAU_OPENAI_MODEL=openai/gpt-4o-mini
TAU_ANTHROPIC_MODEL=anthropic/claude-sonnet-4-20250514
TAU_GOOGLE_MODEL=google/gemini-2.5-pro
TAU_OPENROUTER_MODEL=openrouter/openai/gpt-4.1-mini
TAU_DEEPSEEK_MODEL=deepseek/deepseek-chat
TAU_XAI_MODEL=xai/grok-4
TAU_MISTRAL_MODEL=mistral/mistral-large-3
TAU_GROQ_MODEL=groq/llama-3.3-70b
